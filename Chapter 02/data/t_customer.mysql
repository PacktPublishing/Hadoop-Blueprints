create table t_customer (
	id INT,
	first_name VARCHAR(50),
	last_name VARCHAR(50),
	email VARCHAR(50),
	country VARCHAR(50),
	ip_address VARCHAR(20),
	gender VARCHAR(50),
	userid VARCHAR(50),
	currency VARCHAR(50),
	jobtitle VARCHAR(50),
	companyname VARCHAR(50),
	creditcardnumber VARCHAR(50),
	domainname VARCHAR(1000),
	latitude VARCHAR(50),
	longitude VARCHAR(50)
);
